{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://jira-clone-using-monorepo.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api?test=cron",
      "schedule": "0 0 * * *"
    }
  ]
}
